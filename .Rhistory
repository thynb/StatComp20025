f_i=function(u,a,b){
f1=b
f2=(1-u)^(1/a)
return(f1/f2)
}
n <- 1000
u <- runif(n)
x <- f_i(u,2,2)
hist(x, density = 3,breaks=50, col="red",prob = TRUE) #density histogram of sample
f=function(x,a,b){
x1=x^(-a-1)
return(a*b*x1)
}
y <- seq(2,max(x),0.01)
lines(y, f(y,2,2),col='blue')
m <- 10000
x <- runif(m, min=2, max=4)
theta.hat <- mean(exp(-x)) * 2
print(theta.hat)
